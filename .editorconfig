root = true
[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = space
[*.{php,py}]
indent_size = 4
[*.{js,md,html,css,scss,less,vue}]
indent_size = 2
[Makefile]
indent_style = tab
[{package.json,.travis.yml,*.blade.php}]
indent_style = space
indent_size = 2
